{
  "name": "tintify",
  "version": "1.0.0",
  "description": "Tintify tints your untinted terminal",
  "main": "index.ts",
  "scripts": {
    "dev": "npx nodemon --exec node -r @swc-node/register -r tsconfig-paths/register ./src/index.ts dev",
    "build": "tsc --build",
    "start": "node build/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unarray/Tintify.git"
  },
  "keywords": [
    "color",
    "terminal",
    "ansi",
    "tint"
  ],
  "author": "Unarray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unarray/Tintify/issues"
  },
  "homepage": "https://github.com/Unarray/Tintify#readme",
  "devDependencies": {
    "@bluzzi/eslint-config": "^0.9.0",
    "@swc-node/register": "^1.6.3",
    "@types/node": "^18.15.11",
    "@vitest/ui": "^0.29.8",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  },
  "eslintConfig": {
    "extends": "@bluzzi"
  },
  "dependencies": {
    "dayjs": "^1.11.7"
  }
}
